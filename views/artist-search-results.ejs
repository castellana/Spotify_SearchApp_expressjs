<%- include('./partials/head')%>
<%- include('./partials/nav')%>

<section id="artistsSec">
<% artistResults.forEach(item=> { %>
    <figure class="artistItem">
        <!-- <p>Artist images: <%# JSON.stringify(item.images) %></p> para ver lo que hay dentro-->

        <% if(item.images[0] == undefined){ %>
            <img class="noFoto" src="./img/photo-not-available.png" alt="<%= item.name %>">
        <% } else { %>
            <img src="<%= item.images[0].url %>" alt="">
        <% } %>

        <figcaption>
            <p><%= item.name %></p>

            <a href="/albums/<%= item.id %>/0">View Albums</a>
        </figcaption>
        
    </figure>
    
<% }) %>
</section>
<!-- <a href="<%# artistNextPage %>">More</a> -->

<%- include('./partials/footer')%>