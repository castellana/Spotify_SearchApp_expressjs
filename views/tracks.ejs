<%- include('./partials/header')%>

<h3>Tracks</h3>
<%#JSON.stringify(tracksResults) %>
<% tracksResults.forEach(trackItem=> { %>

        <% if(trackItem.preview_url[0] == undefined){ %>
            <img src="./img/no-picture-available.jpg" alt="">
        <% } else { %>
            <p><%= trackItem.name %></p>
            <audio src=<%= trackItem.preview_url %> controls ></audio>
            
        <% } %>
        


<% }) %>

<%- include('./partials/footer')%>